#!/bin/bash
# Debut: verifica si está corriendo Receipt y lo ejecuta si no está corriendo
. Helpers

if [ $# -lt 1 ]
then
        echo "Uso : $0 <<comando a ejecutar>>"
        exit
fi

case "$1" in

$RECEPT)  initProcess $1
    ;;
$FSOLDES)  initProcess $1
    ;;
$CDOSSIERE)  initProcess $1
    ;;
*) echo "El comando $1 no existe"
   ;;
esac
